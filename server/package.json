    "drizzle-orm": "^0.36.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "postgres": "^3.4.4",
    "selfsigned": "^5.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/archiver": "^7.0.0",
    "@types/cors": "^2.8.17",
